| dep_id | mini |
+--------+------+
|     10 | 1100 |
|     20 | 1200 |
+--------+------+
31. select emp_name, salary from employee
    -> where salary between ( select avg(salary) from employee
    -> group by position
    -> having position = '사원') and ( select avg(salary) from employee
    -> group by position
    -> having position = '과장');
