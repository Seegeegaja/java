create table dept(
dep_id int primary key,
dep_name varchar(10) ,
loc varchar(10) not null );

insert into dept
(dep_id , dep_name, loc)
values (10 , '총무부', '서울'),
(20 , '영업부', '부산'),
(30 , '전산부', '대전'),
(40 , '자재부', '광주') ;



create table salary_grade(
sal_grade_id int primary key,
min_sal int ,
max_sal int );


insert into salary_grade(
sal_grade_id, min_sal, max_sal)
values (1,8001,10000),
(2,6001,8000),
(3,4001,6000),
(4,2001,4000),
(5,1001,2000);

create table employee(
emp_id int primary key auto_increment,
emp_name varchar(10) not null,
dep_id int,
position varchar(10) not null,
salary int default 0 ,
hire_date date,
jumin_num char(13) unique,
phone_num varchar(15) not null,
mgr_emp_no int,
foreign key (dep_id) references dept (dep_id) on update cascade on delete cascade
);

insert into employee
(emp_name, dep_id, position, salary, 
hire_date, jumin_num, phone_num, mgr_emp_no) 
